DROP TABLE IF EXISTS "ParadaDeRecorridos";
CREATE TABLE "ParadaDeRecorridos" ("_id" INTEGER PRIMARY KEY  NOT NULL , "idRecorrido" INTEGER NOT NULL UNIQUE , "idParada" INTEGER NOT NULL UNIQUE , FOREIGN KEY (idRecorrido) REFERENCES Recorridos (_id), FOREIGN KEY (idParada) REFERENCES  Paradas (_id));
DROP TABLE IF EXISTS "Paradas";
CREATE TABLE "Paradas" ("_id" INTEGER PRIMARY KEY  NOT NULL , "nombreCalle" TEXT, "altura" INTEGER, "fechaHora" TEXT, "linea" INTEGER, "cartel" TEXT, "idUsuario" INTEGER NOT NULL UNIQUE , FOREIGN KEY (idUsuario) REFERENCES Usuarios (_id));

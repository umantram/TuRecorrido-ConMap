DROP TABLE IF EXISTS "Cortes";
CREATE TABLE "Cortes" ("_id" INTEGER PRIMARY KEY  NOT NULL , "nombreCorte" TEXT, "altura" INTEGER, "fechaHora" TEXT, "linea" INTEGER, "cartel" TEXT, "nombreCalle" TEXT, "idUsuario" INTEGER NOT NULL UNIQUE , "idRecorrido" INTEGER NOT NULL UNIQUE, FOREIGN KEY (idUsuario) REFERENCES Usuarios (_id) , FOREIGN KEY (idRecorrido) REFERENCES Recorridos (_id));
DROP TABLE IF EXISTS "LineasColectivos";
CREATE TABLE "LineasColectivos" ("_id" INTEGER PRIMARY KEY  NOT NULL  DEFAULT (null) ,"cartel" CHAR,"linea" INTEGER NOT NULL );
